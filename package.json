{
  "name": "random-fixtures",
  "version": "0.0.3",
  "description": "Generated random quotes, addresses, numbers, etc",
  "main": "dist/index.js",
  "type": "module",
  "workspaces": [
    "packages/core",
    "packages/ancient",
    "packages/address",
    "packages/chuck-norris"
  ],
  "scripts": {
    "build": "npm run copy-readme && npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "test:watch": "npm run test:watch --workspaces --if-present",
    "dev": "npm run dev --workspaces --if-present",
    "clean": "rm -rf packages/**/dist",
    "copy-readme": "node scripts/copy-readme.js",
    "remove-readme": "node scripts/remove-readme.js",
    "bump": "node scripts/bump-version.js",
    "release": "npm run bump && npm run release-no-bump",
    "release-no-bump": "npm run build && npm publish --access public --workspaces --if-present && npm run remove-readme"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.29.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.5.1",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "22.17.1",
    "npm": "11.4.2"
  }
}
